{
    "manifest_version": 2,
    "name": "Phishing Link Detector",
    "version": "1.0",
    "description": "Detect phishing links in emails.",
    "permissions": [
        "activeTab",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content_script.js"]
        }
    ]
  }